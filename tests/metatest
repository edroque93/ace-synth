#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1ac8350 .scope module, "metatest_tb" "metatest_tb" 2 3;
 .timescale 0 0;
    .scope S_0x1ac8350;
T_0 ;
    %vpi_call 2 6 "$display", "These tests should all work";
    %vpi_call 2 12 "$display", "These tests should all fail";
    %vpi_call 2 13 "$write", "- Assertion failed: ";
    %vpi_call 2 13 "$display", "Zero is different than zero";
    %vpi_call 2 14 "$write", "- Assertion failed: ";
    %vpi_call 2 14 "$display", "One is different than one";
    %vpi_call 2 15 "$write", "- Assertion failed: ";
    %vpi_call 2 15 "$display", "Zero equals one";
    %vpi_call 2 16 "$write", "- Assertion failed: ";
    %vpi_call 2 16 "$display", "One plus one is different than two";
    %vpi_call 2 17 "$finish";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "metatest_tb.v";
